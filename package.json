{
  "name": "room3coffee",
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "clean": "rimraf dist",
    "sass": "node-sass -o css/ css/",
    "copyjs": "copyfiles -f node_modules/bootstrap/dist/js/bootstrap.bundle.js js",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts",
    "babel": "babel js/* -d js",
    "imagemin": "imagemin img/* -o dist/img",
    "start": "npm run watch:all",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html && usemin aboutus.html -d dist --htmlmin -o dist/aboutus.html",
    "build": "npm run clean && npm run copyfonts && npm run copyjs && npm run babel && npm run imagemin && npm run usemin",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "watch:all": "parallelshell \"npm run watch:scss\" \"npm run lite\""
  },
  "author": "Sharon Lewis, Marc Perez, Ryan Zierman",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^5.2.3",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "copyfiles": "^2.2.0",
    "imagemin-cli": "^5.1.0",
    "node-sass": "^8.0.0",
    "onchange": "^7.0.0",
    "parallelshell": "3.0.1",
    "rimraf": "^3.0.2",
    "usemin-cli": "^0.6.0"
  }
}
